.container {
    width: 750rpx;
    /*height: 1545*@wu;*/
    background-color: #f4f4f4;
    text-align: left;
    padding-bottom:280rpx;
}
.head {
    width: 750rpx;
    height: 524rpx;
    position: relative;
}
.head image {
    width: 750rpx;
    height: 524rpx;
    position: absolute;
}
.head .wider {
    position: absolute;
    border: 20rpx solid #fff;
    border-bottom: none;
    width: 653rpx;
    height: 385rpx;
    box-sizing: border-box;
    margin: 140rpx 49rpx 0 49rpx;
}
.head .wider .wide {
    width: 581rpx;
    height: 353rpx;
    border: 2rpx solid #fff;
    border-bottom: none;
    box-sizing: border-box;
    margin: 10rpx auto;
}
.head .wider .wide .headCont .headContTop {
    padding: 0 28rpx;
}
.head .wider .wide .headCont .headContTop .hctName {
    height: 54rpx;
    line-height: 66rpx;
    font-size: 30rpx;
    font-weight: 600;
    text-align: left;
    color: #ffffff;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 100%;
}
.head .wider .wide .headCont .headContTop .hctTime {
    height: 60rpx;
    font-size: 22rpx;
    font-weight: 600;
    line-height: 60rpx;
    text-align: left;
    color: #ffffff;
    border-bottom: 2rpx dashed #fff;
}
.head .wider .wide .headCont .headContCent {
    padding: 0 28rpx;
    height: 180rpx;
    line-height: 180rpx;
    font-size: 54rpx;
    font-weight: 600;
    text-align: left;
    color: #ffffff;
    text-align: center;
    overflow: hidden;
}
.head .wider .wide .headCont .headContBot {
    padding: 0 28rpx;
    height: 66rpx;
    font-size: 24rpx;
    font-weight: 600;
    line-height: 2;
    color: #ffffff;
}
.head .wider .wide .headCont .headContBot text:last-child{
    float: right;
}
.leaguer {
    width: 750rpx;
    height: 80rpx;
    text-align: left;
    line-height: 80rpx;
    padding: 0 30rpx;
    box-sizing: border-box;
    background-color: #fefdfd;
}
.leaguer view {
    height: 80rpx;
    text-align: left;
    line-height: 80rpx;
}
.direction,
.detail {
    background-color: #ffffff;
    margin-top: 20rpx;
}
.direction .direTop,
.detail .direTop {
    margin: 0 30rpx;
    width: 690rpx;
    height: 92rpx;
    border-bottom: 2rpx solid #eee;
    display: flex;
    box-sizing: border-box;
    line-height: 92rpx;
}
.direction .direTop .pic,
.detail .direTop .pic {
    margin-right: 16rpx;
}
.direction .direTop .pic image,
.detail .direTop .pic image {
    width: 32rpx;
    height: 32rpx;
    margin-top: 13px;
}
.direction .dtText,
.detail .direTop .txt {
    font-size: 30rpx;
    font-weight: 500;
    color: #222222;
}
.direCont {
    padding: 28rpx;
}
.direCont view {
    display: flex;
}
.direCont view .tit {
    font-size: 28rpx;
    width: 120rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: left;
    color: #999999;
    margin-right: 30rpx;
    display: inline-block;
}
.direCont view .txt {
    font-size: 28rpx;
    line-height: 60rpx;
    text-align: left;
    color: #222222;
    width: 480rpx;
    overflow: hidden;
    display: inline-block;
}
.detail {
    padding-bottom: 60rpx;
}
.detail .detailCont {
    width: 750rpx;
    padding: 0 28rpx;
    box-sizing: border-box;
    overflow: hidden;
}
.detail .detailCont .card {
    padding-top: 40rpx;
}
.detail .detailCont .dcPic {
    width: 694rpx;
    height: 417rpx;
    border-radius: 14rpx;
}
.detail .detailCont .dcPic image {
    width: 694rpx;
    height: 417rpx;
    border-radius: 14rpx;
}
.detail .detailCont .describe {
    font-size: 30rpx;
    font-weight: 500;
    line-height: 1.3;
    text-align: justify;
    color: #222222;
    padding: 20rpx 0;
}
/*领取分享按钮*/
.freeReceive {
    height: 276rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    background-color: rgba(255,255,255,0.9);
}
.freeReceive button {
    margin-top: 20rpx;
    background-color: #fff;
    font-size: 36rpx;
    font-weight: 500;
    color: #222222;
    border-radius: 10rpx;
    width: 350rpx;
    /*color: #999999;*/
}
/*按钮点击效果*/
.freeReceive .receive {
    width: 750rpx;
    background-color: #fd6759;
    color: #fff;
}
.freeReceive .receive2 {
    width: 280rpx;
    background-color: transparent;
    color: #fe8a3d;
    float:left;
    margin-left:0;
    margin-right:44rpx;
    border:1px solid #fe8a3d;
}
.freeReceive .receive.button-hover {
    background-color: #ff6f27;
}
.freeReceive .share.button-hover {
    border: 4rpx solid #fe8a3d;
    color:#fe8a3d;
}

.freeReceive .hadDraw{
    background-color: rgb(155,155,155);
    width: 750rpx;
    color: #fff;
}
.backImg{
    width: 425rpx;
    height: 264rpx;
    margin-top: -105rpx;
}
.backCont{
    width: 100%;
    overflow: hidden;
    margin-top: 74rpx;
}
.backContBox{
    white-space: nowrap;
    overflow: hidden;
    margin-bottom: 30rpx;
    height: 42rpx;
}
.backContBoxTit{
    width: 300rpx;
    text-align: right;
    float:left;
    font-size: 28rpx;
}
.backContBoxVal{
    float: left;
    color: rgb(34,34,34);
    font-size: 28rpx;
    line-height:38rpx;
    padding-left: 5rpx;
}
.backClose{
    position: absolute;
    left: 50%;
    margin-left: -28rpx;
    top:800rpx;
    width: 56rpx;
    height: 56rpx;
}
/*适用商家*/
.seller {
    width: 100%;
    display: flex;
    flex-direction: column;
    margin-top: 0;
}

.seller .sellerCont{
    width: 750rpx;
    padding: 30rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
}
.seller .sellerCont .sellLogo{
    width: 102rpx;
    height: 100rpx;
    border-radius: 10rpx;
    background-color: #ffffff;
    border: solid 1rpx #eeeeee;
    margin-right: 25rpx;
}
.seller .sellerCont .sellLogo image{
    width: 88rpx;
    height: 90rpx;
    margin-top: 3px;
}
.seller .sellerCont .sellDetail {
    width: 600rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    position: relative;
}
.seller .sellerCont .sellDetail .sellName{
    width: 600rpx;
    font-size: 32rpx;
    font-weight: 500;
    line-height: 1.5;
    text-align: left;
    color: #222222;
}
.seller .sellerCont .sellDetail .sellNature{
    width: 600rpx;
    display: flex;
}
.seller .sellerCont .sellDetail .sellNature .nature{
    font-size: 26rpx;
    width: 118rpx;
    display: inline-block;
    border-right: 1px solid #eeeeee;
    font-weight: 500;
    line-height: 1.85;
    text-align: left;
    color: #fe8f45;
    margin-right: 10px;
}
.seller .sellerCont .sellDetail .sellNature .addr{
    display: inline-block;
    width: 450rpx;
    overflow: hidden;
    font-size: 26rpx;
    font-weight: 500;
    line-height: 1.85;
    text-align: left;
    color: #999999;
}
.seller .direTop{
    width: 750 rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
.seller .dtLeft{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
.seller .dtLeft .pic{
    display: inline-block;
}
.seller .dtRight{
    display: inline-block;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
.seller .dtRight .dtText{
    margin-right: 30 rpx;
}
.seller .dtRight .toPic{
    display: inline-block;
}
.seller .dtRight .toPic image{
    width: 15rpx;
    height: 28rpx;
}
.phone{
    position: absolute;
    top:10rpx;
    right: 48rpx;
    width: 100rpx;
    height: 80rpx;
    border-left: 2rpx solid rgb(238,238,238);
}
.phone image{
    width: 44rpx;
    height: 44rpx;
    margin-left: 50rpx;
    margin-top:16rpx;

}
.coinNum{
    width: 60rpx;
    height: 80rpx;
    display: inline-block;
    padding: 0;
    line-height: 80rpx;
    vertical-align:middle;
    border:1rpx solid rgb(204,204,204);
    margin:0 -2rpx;
    text-align:center;
}
.coinNumText{
    width: 140rpx!important;
    display: inline-block;
    border:1rpx solid rgb(204,204,204);
    vertical-align:middle;
    text-align: center!important;
}
.coinLabel{
    margin-left: 90rpx;
    margin-right: 20rpx;
}
.payNumber{
    font-size: 72rpx;
}
.coinImg{
    width: 330rpx;
    height: 260rpx;
    margin-top: -100rpx;
}