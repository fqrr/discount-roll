page{
    height: 100%;
}
.container {
    width: 750rpx;
    height: 100%;
    /*height: 1545*@wu;*/
    background-color: #f4f4f4;
    text-align: left;
}
.sellerLists{
    background-color: #fff;
}
/*适用商家*/
.seller {
    width: 750rpx;
    background-color: #fff;
    box-sizing: border-box;
}
.seller .sellerCont{
    /*display: flex;*/
    width: 750 rpx;
    padding:30 rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
    border-bottom:  solid 2rpx #f4f4f4;
    /*border-bottom:  solid 1rpx red;*/
}
.seller .sellerCont .sellLogo{
    width: 102rpx;
    height: 100rpx;
    border-radius: 10rpx;
    background-color: #ffffff;
    border: solid 1rpx #eeeeee;
    margin-right: 25rpx;
}
.seller .sellerCont .sellLogo image{
    width: 88rpx;
    height: 90rpx;
    margin-top: 3px;
}
.seller .sellerCont .sellDetail {
    width: 600rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
}
.seller .sellerCont .sellDetail .sellName{
    width: 600rpx;
    font-size: 32rpx;
    font-weight: 500;
    line-height: 1.5;
    text-align: left;
    color: #222222;
}
.seller .sellerCont .sellDetail .sellNature{
    width: 600rpx;
    display: flex;
}
.seller .sellerCont .sellDetail .sellNature .nature{
    font-size: 26rpx;
    width: 118rpx;
    display: inline-block;
    border-right: 1px solid #eeeeee;
    font-weight: 500;
    line-height: 1.85;
    text-align: left;
    /*color: #fe8f45;*/
    color: #fe8a3d;
    margin-right: 10px;
}
.seller .sellerCont .sellDetail .sellNature .addr{
    display: inline-block;
    width: 450rpx;
    overflow: hidden;
    font-size: 26rpx;
    font-weight: 500;
    line-height: 1.85;
    text-align: left;
    color: #999999;
}
