page{
    background-color: rgb(244,245,250);
}
.swiper{
    overflow: hidden;
    position: fixed;
    z-index: 100;
    width: 100%;
}
.swiper-tab{
    font-size: 28rpx;
    display: flex;
    width: 100%;
    background-color: rgb(244,245,250);
}
.swiper-tab-list{
    flex: calc(1/3);
    height: 68rpx;
    text-align: center;
    line-height: 68rpx;
    overflow: hidden;
    /*background-color:#ffffff;*/
    color:rgb(153,153,153);
    box-sizing: border-box;
    /*border-right: 1px solid rgba(254,138,61,0.5);*/
}
.codeTit{
    height: 80rpx;
    line-height: 80rpx;
    margin-bottom: 20rpx;
    position: relative;
}
.codeTitLeft{
    position: absolute;
    left: 0;
    top:40rpx;
    height: 1px;
    background-color: rgb(253,103,89);
    width: 712rpx;
    z-index: 1;
}
.codeTitText{
    position: absolute;
    left: 50%;
    top:0;
    width: 350rpx;
    margin-left: -175rpx;
    color: rgb(253,103,89);
    text-align: center;
    font-size: 32rpx;
    background-color: rgb(244,245,250);
    z-index: 10;
    box-sizing: border-box;
    padding-left: 14rpx;
}
.on{
    /*background-color: #970c0f;*/
    color: rgb(253,103,89);
}
.on .bottomColor{
    width: 36rpx;
    height: 6rpx;
    background-color: rgb(253,103,89);
    margin: -6rpx auto 0 auto;
}
.container {
    width: 750rpx;
    /*height: 1545*@wu;*/
    /*overflow: hidden;*/
    /*background-color: #970c0f;*/
    text-align: left;

}
.head {
    width: 750rpx;
    height: 420rpx;
    position: relative;
}
.head image {
    width: 750rpx;
    height: 420rpx;
    position: absolute;
}

/*第一个卷*/
.listBoxLeft{
    width: 350rpx;
    height: 300rpx;
    position: relative;
    z-index: 10;
    display: inline-block;
    text-align: center;
}
.listBoxRight{
    width: 350rpx;
    height: 300rpx;
    position: relative;
    z-index: 10;
    display: inline-block;
    margin-left: 10rpx;
    text-align: center;
}
.listBoxLeftBjImg{
    position: relative;
    z-index: 11;
    width: 350rpx;
    height: 300rpx;
}
.listBoxLeftBox{
    width: 350rpx;
    height: 300rpx;
    position: absolute;
    top:0;
    left: 0;
    z-index: 13;
}
.shopImg{
    width:140rpx;
    height:140rpx;
    display:block;
   /* border:2rpx solid rgb(244,245,250);*/
    margin:14rpx auto 0 auto;
}
.goodCode{
    position: absolute;
    right: -4rpx;
    top: 14rpx;
    font-size: 20rpx;
    transform: rotate(45deg);
    color: #ffffff;
}
.shopName{
    font-size: 28rpx;
    color: rgb(192,177,115);
    width: 350rpx;
    height: 48rpx;
    line-height: 48rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    margin-top: 20rpx;
}
.shopTit{
    font-size: 32rpx;
    width: 350rpx;
    height: 48rpx;
    line-height: 48rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    color: rgb(51,51,51);
    margin-top: 11rpx;
}
.notDisplay{
    display: inline-block;
}

.cardList {
    /*background-color: #970c0f;*/
    height: 590rpx;
    position: relative;
}
.list {
    position: relative;
    display: block;
    /*margin-bottom: 36rpx;*/
    width: 712rpx;
    height: auto;
}
.list:nth-child(1) {
    margin-top: 30rpx;
}
.moreIcon{
    width: 26rpx;
    height: 26rpx;
    border-radius: 50%;
    text-align: center;
    font-size: 14rpx;
    line-height: 26rpx;
    color: #ffffff;
    background-color: rgb(153,153,153);
    display: inline-block;
    vertical-align: middle;
    margin-left: 10rpx;
    margin-top: -4rpx;
}
.cardLevels{
    font-size: 24rpx;
    color:#970c0f;
}
.more{
    color: rgb(153,153,153);
    font-size: 28rpx;
    text-align: center;
}
.swiperEx{
    height: 68rpx;
    overflow: hidden;
}