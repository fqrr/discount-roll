/*没有卡券*/
.noCard{
    width: 750 rpx;
    height: 260 rpx;
    margin-top: 282rpx
}
.noCard image{
    display: block;
    width: 180rpx;
    height: 180rpx;
    margin: 0 auto;
}
.noCard .txt{
    text-align: center;
    font-size: 36rpx;
    font-weight: 500;
    line-height: 1.33;
    color: #999999;
    letter-spacing: 0.5rpx;
    margin-top: 68rpx
}


/*头部登陆*/
.topLogin{
    height: 128rpx;
    width: 750rpx;
    background-color: #ffffff;
    overflow: hidden;
    line-height: 30rpx;
}
.topLogin .tl-left{
    float: left;
    width: 240rpx;
    height: 80rpx;
    line-height: 80rpx;
    padding-left:28rpx;
}
.topLogin .tl-left .leftChild{
    font-size: 24rpx;
    line-height:60rpx;
    color: rgb(51,51,51);
}
.topLogin .tl-left image{
    width: 18rpx;
    height: 20rpx;
    margin-right: 10rpx;
}
.topLogin .tl-right{
    float: right;
    width: 180rpx;
    height: 80rpx;
    line-height: 128rpx;
}
.topLogin .backOrChange{
    display: inline-block;
    font-size: 24rpx;
    color: rgb(51,51,51);
    box-sizing: border-box;
    padding-right: 10rpx;
}
.userTit{
    color:rgb(253,103,89);
}
.moreIcon{
    width: 26rpx;
    height: 26rpx;
    border-radius: 50%;
    text-align: center;
    font-size: 14rpx;
    line-height: 26rpx;
    color: #ffffff;
    background-color: rgb(153,153,153);
    display: inline-block;
    vertical-align: middle;
    margin-left: 10rpx;
    margin-top: -4rpx;
}
.more{
    color: rgb(153,153,153);
    font-size: 28rpx;
    text-align: center;
    margin-top: 20rpx;
}
